    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/pageform</url-pattern>
    </servlet-mapping>
    
<!--    <servlet-mapping>
        <servlet-name>s123</servlet-name>
        <url-pattern>/s</url-pattern>
    </servlet-mapping>
    -->
    
    <session-config>
        
